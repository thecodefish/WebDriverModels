        <tags>webdriver model</tags>
        <dependencies>
            <dependency id="Castle.Core" version="3.3.0" />
            <dependency id="Selenium.WebDriver" version="2.41.0" />
            <dependency id="Selenium.Support" version="2.41.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="..\WebDriverModels\bin\Release\WebDriverModels.dll" target="lib\net45\WebDriverModels.dll" />
    </files>
</package>